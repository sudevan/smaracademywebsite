                        '<div class="mfp-close"></div>'+
                        '<iframe class="mfp-iframe" frameborder="0" allowfullscreen></iframe>'+
                        '</div>', // HTML markup of popup, `mfp-close` will be replaced by the close button

                    patterns: {
                        youtube: {
                        index: 'youtube.com/', // String that detects type of video (in this case YouTube). Simply via url.indexOf(index).

                        id: 'v=', // String that splits URL in a two parts, second part should be %id%
                        // Or null - full URL will be returned
                        // Or a function that should return %id%, for example:
                        // id: function(url) { return 'parsed id'; }

                        src: '//www.youtube.com/embed/%id%?autoplay=1&rel=0' // URL that will be set as a source for iframe.
                        }
                    }
                }
            });

            // Select and loop the container element of the elements you want to equalise
            $('.testi-slider').each(function(){  
              
              // Cache the highest
              var highestBox = 0;
              
              // Select and loop the elements you want to equalise
              $('.box', this).each(function(){
                
                // If this box is higher than the cached highest then store it
                if($(this).height() > highestBox) {
                  highestBox = $(this).height(); 
                }
              
              });  
                    
              // Set the height of all those children to whichever was highest 
              $('.box',this).height(highestBox);
                            
            });
        });


    </script>
  <!-- common js - adding to footer to reduce a http request -->
<script>
    $(window).scroll(function() {
        var scroll = $(window).scrollTop();
        if (scroll >= 100) {
            $(".header").addClass("scrolled");
        } else {
            $(".header").removeClass("scrolled");
        }
    });

    $(document).ready(function(){
        setTimeout(function(){
            $('.preloader').fadeOut();
            $('.fixed-whatsapp').addClass('show-icon');
        }, 300);
        
        $(".mobile-icon").click(function(){
            $(".menu-sect").addClass("open");
        })
        $(".menu-sect .close").click(function(){
            $(".menu-sect").removeClass("open");
        });
        $('.get_started').on('click', function(e){
            e.preventDefault();
            $('html, body').css('overflow','hidden');
            $('.enquiry-popup').fadeIn();
        });
        $('.enq-close, .enquiry-bg').on('click', function(){
            $('.enquiry-popup').fadeOut();        
            $('html, body').css('overflow','auto');
        });

        $('.menu-sect .menu li.menu-item-has-children>a').on('click', function(e){
            e.preventDefault();
            if($(this).hasClass('active')){
                $(this).parent().find('ul.sub-menu').slideUp();
                $(this).removeClass('active');
            } else {
                $(this).parent().find('ul.sub-menu').slideDown();
                $(this).addClass('active');
            }
        });

        //Enquiry form

        var ipinfo;

        $.getJSON("https://ipinfo.io/", function (data) {
            $("#enqForm input[name=ipinfo], #enqForm2 input[name=ipinfo]").val("City: " + data.city + " ,County: " + data.country + " ,IP: " + data.ip + " ,Location: " + data.loc + " ,Organisation: " + data.org + " ,Postal Code: " + data.postal + " ,Region: " + data.region + "");
        })
        jQuery('#enqForm2').validate({

            submitHandler: function(form) {
                $.ajax({
                    type: "POST",
                    url: 'https://admin.mytuitionapp.com/wp-json/web/v1/submitenquery',
                    data: $(form).serialize(),
                    dataType: "json",
                    beforeSend: function(){
                        $("input[name=submit]", form).attr('disabled', 'disabled');
                        $("div.loading", form).show();
                        $("div.status", form).hide();
                    },
                    success: function(result){
                        fail = false;
                        msg = '';
                        $.each(result, function (key, value) { 
                            if(key=="status"){
                                if(value==true){
                                    fail =false;
                                } else {
                                    fail =true;
                                }
                            }
                            if(key=='message'){
                                msg = value;
                            }

                        });
                        if (!fail) {
                            $(form)[0].reset()
                            window.location.href = 'thank-you/index.html';
                        }
                        else {
                            $("div.loading", form).hide();
                            $("input[name=submit]", form).removeAttr('disabled');
                            $("div.status", form).html(msg).show();
                        }
                    }
                });
            },
            errorPlacement: function(error, element) {
                var placement = $(element).data('error');
                if (placement) {
                    $(placement).append(error)
                } else {
                    error.insertAfter(element);
                }
            },
            errorElement: 'span',
        });

        jQuery('#enqForm').validate({

            submitHandler: function(form) {
                $.ajax({
                    type: "POST",
                    url: 'https://admin.mytuitionapp.com/wp-json/web/v1/submitenquery',
                    data: $(form).serialize(),
                    dataType: "json",
                    beforeSend: function(){
                        $("input[name=submit]", form).attr('disabled', 'disabled');
                        $("div.loading", form).show();
                        $("div.status", form).hide();
                    },
                    success: function(result){
                        fail = false;
                        msg = '';
                        $.each(result, function (key, value) { 
                             if(key=="status"){
                                 if(value==true){
                                    fail =false;
                                 } else {
                                    fail =true;
                                 }
                             }
                             if(key=='message'){
                                 msg = value;
                             }

                        });
                        if (!fail) {
                            $(form)[0].reset()
                            window.location.href = 'thank-you/index.html';
                        }
                        else {
                            $("div.loading", form).hide();
                            $("input[name=submit]", form).removeAttr('disabled');
                            $("div.status", form).html(msg).show();
                        }
                    }
                });
            },
            errorPlacement: function(error, element) {
                var placement = $(element).data('error');
                if (placement) {
                    $(placement).append(error)
                } else {
                    error.insertAfter(element);
                }
            },
            errorElement: 'span',
        });
        
    });
</script>


