<script  defer src='https://mytuitionapp.com/wp-content/themes/mtapp/js/validate.js'></script>
<script  defer src='https://mytuitionapp.com/wp-content/themes/mtapp/js/additional-methods.min.js'></script>
     <script>
        $(document).ready(function(){
        });


    </script>
  <!-- common js - adding to footer to reduce a http request -->
<script>
    $(window).scroll(function() {
        var scroll = $(window).scrollTop();
        if (scroll >= 100) {
            $(".header").addClass("scrolled");
        } else {
            $(".header").removeClass("scrolled");
        }
    });

    $(document).ready(function(){
        setTimeout(function(){
            $('.preloader').fadeOut();
            $('.fixed-whatsapp').addClass('show-icon');
        }, 300);
        
        $(".mobile-icon").click(function(){
            $(".menu-sect").addClass("open");
        })
        $(".menu-sect .close").click(function(){
            $(".menu-sect").removeClass("open");
        });
        $('.get_started').on('click', function(e){
            e.preventDefault();
            $('html, body').css('overflow','hidden');
            $('.enquiry-popup').fadeIn();
        });
        $('.enq-close, .enquiry-bg').on('click', function(){
            $('.enquiry-popup').fadeOut();        
            $('html, body').css('overflow','auto');
        });

        $('.menu-sect .menu li.menu-item-has-children>a').on('click', function(e){
            e.preventDefault();
            if($(this).hasClass('active')){
                $(this).parent().find('ul.sub-menu').slideUp();
                $(this).removeClass('active');
            } else {
                $(this).parent().find('ul.sub-menu').slideDown();
                $(this).addClass('active');
            }
        });

        //Enquiry form

        var ipinfo;

        $.getJSON("https://ipinfo.io/", function (data) {
            $("#enqForm input[name=ipinfo], #enqForm2 input[name=ipinfo]").val("City: " + data.city + " ,County: " + data.country + " ,IP: " + data.ip + " ,Location: " + data.loc + " ,Organisation: " + data.org + " ,Postal Code: " + data.postal + " ,Region: " + data.region + "");
        })
        jQuery('#enqForm2').validate({

            submitHandler: function(form) {
                $.ajax({
                    type: "POST",
                    url: 'https://admin.mytuitionapp.com/wp-json/web/v1/submitenquery',
                    data: $(form).serialize(),
                    dataType: "json",
                    beforeSend: function(){
                        $("input[name=submit]", form).attr('disabled', 'disabled');
                        $("div.loading", form).show();
                        $("div.status", form).hide();
                    },
                    success: function(result){
                        fail = false;
                        msg = '';
                        $.each(result, function (key, value) { 
                            if(key=="status"){
                                if(value==true){
                                    fail =false;
                                } else {
                                    fail =true;
                                }
                            }
                            if(key=='message'){
                                msg = value;
                            }

                        });
                        if (!fail) {
                            $(form)[0].reset()
                            window.location.href = '../thank-you/index.html';
                        }
                        else {
                            $("div.loading", form).hide();
                            $("input[name=submit]", form).removeAttr('disabled');
                            $("div.status", form).html(msg).show();
                        }
                    }
                });
            },
            errorPlacement: function(error, element) {
                var placement = $(element).data('error');
                if (placement) {
                    $(placement).append(error)
                } else {
                    error.insertAfter(element);
                }
            },
            errorElement: 'span',
        });

        jQuery('#enqForm').validate({

            submitHandler: function(form) {
                $.ajax({
                    type: "POST",
                    url: 'https://admin.mytuitionapp.com/wp-json/web/v1/submitenquery',
                    data: $(form).serialize(),
                    dataType: "json",
                    beforeSend: function(){
                        $("input[name=submit]", form).attr('disabled', 'disabled');
                        $("div.loading", form).show();
                        $("div.status", form).hide();
                    },
                    success: function(result){
                        fail = false;
                        msg = '';
                        $.each(result, function (key, value) { 
                             if(key=="status"){
                                 if(value==true){
                                    fail =false;
                                 } else {
                                    fail =true;
                                 }
                             }
                             if(key=='message'){
                                 msg = value;
                             }

                        });
                        if (!fail) {
                            $(form)[0].reset()
                            window.location.href = '../thank-you/index.html';
                        }
                        else {
                            $("div.loading", form).hide();
                            $("input[name=submit]", form).removeAttr('disabled');
                            $("div.status", form).html(msg).show();
                        }
                    }
                });
            },
            errorPlacement: function(error, element) {
                var placement = $(element).data('error');
                if (placement) {
                    $(placement).append(error)
                } else {
                    error.insertAfter(element);
                }
            },
            errorElement: 'span',
        });
        
    });
</script>
